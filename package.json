{
  "name": "synesthizer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean-audio": "for file in public/*.aiff; do ffmpeg -y -i $file -af silenceremove=start_periods=1:start_duration=0.1:start_threshold=-60dB ${file%%.aiff}.mp3; done",
    "dev": "next dev --turbopack",
    "export": "next build",
    "fmt": "biome format --write && biome check --write",
    "lint": "biome check && bun test",
    "start": "next start"
  },
  "dependencies": {
    "next": "15.3.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "tone": "^15.1.22",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/bun": "^1.2.22",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  }
}
